            <br>
            <button type="button" class="btn btn-light">Players name (action to card) </button>

        </div>
    </section>
    <div class="wait list">
        <h4>
            <center>Wait List</center>
        </h4>
        </ul>
        <button type="button" class="btn btn-light">Players name (action to card) </button>
        <span class="add">
            <button type="button" class="btn btn-light">Add</button>
            <span class="drafted">
                <button type="button" class="btn btn-light">Drafted</button>
            </span>
            <br>
            <button type="button" class="btn btn-light">Players name (action to card) </button>
            <span class="add">
                <button type="button" class="btn btn-light">Add</button>
                <span class="drafted">
                    <button type="button" class="btn btn-light">Drafted</button>
                </span>
                <br>
                <button type="button" class="btn btn-light">Players name (action to card) </button>
                <span class="add">
                    <button type="button" class="btn btn-light">Add</button>
                    <span class="drafted">
                        <button type="button" class="btn btn-light">Drafted</button>
                    </span>
                    <br>
                    <button type="button" class="btn btn-light">Players name (action to card) </button>
                    <span class="add">
                        <button type="button" class="btn btn-light">Add</button>
                        <span class="drafted">
