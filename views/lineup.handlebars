            <button type="button" class="btn btn-light">Players name (action to card) </button>
            <br>
            <button type="button" class="btn btn-light">Players name (action to card) </button>
            <br>
            <button type="button" class="btn btn-light">Players name (action to card) </button>

        </div>
    </section>

    <section>
        <div class="bench">
            <h4>
                <center>Bench</center>
            </h4>
            <ul id="benchMenu">
                <li>Player Name</li>
                <li>Position</li>
                <li>Drafting Position</li>
                <li>Ranking</li>
            </ul>
            <button type="button" class="btn btn-light">Players name (action to card) </button>
            <br>
            <button type="button" class="btn btn-light">Players name (action to card) </button>
            <br>
            <button type="button" class="btn btn-light">Players name (action to card) </button>
