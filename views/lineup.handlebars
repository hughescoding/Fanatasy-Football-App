                                <span class="drafted">
                                    <button type="button" class="btn btn-light">Drafted</button>
                                </span>

    </div>
    </section>

    <section class="activePlayers">
        <div class="availablePlayers">
            <center>
                <h4>Available Players</h4>
            </center>
            <div id="searchform">
                <label class="search">
                    <input id="idSearchBox" class="ui-autocomplete-input DefaultMessage" value="Player Search" msg="Player Search"
                        autocomplete="off">
                </label>
                <div class="DropDown">
                    <select id="cheatSheetOption" ng-model="queueFilter" ng-change="refillQueue()" class="ng-pristine ng-valid ng-touched">
                        <option value="TopEach">Top player from each position</option>
                        <option value="Suggestions" selected="selected">Suggested players to consider</option>
                        <option ng-repeat="rankingsArray" value="nfl_Overall">Overall Rankings</option>
                        <option ng-repeat="rankingsArray" value="nfl_QB">Quarterback (QB)</option>
                        <option ng-repeat="rankingsArray" value="nfl_RB">Running Back (RB)</option>
                        <option ng-repeat="rankingsArray" value="nfl_WR">Wide Receiver (WR)</option>
                        <option ng-repeat="rankingsArray" value="nfl_TE">Tight End (TE)</option>
                        <option ng-repeat="rankingsArray" value="nfl_K">Kicker (K)</option>
                        <option ng-repeat="rankingsArray" value="nfl_DEF">Defense (DEF)</option>
                    </select>
                    <br>
                    <br>
                    <span class="players">
                        <button type="button" class="btn btn-light">Players name (action to card) </button>
                        <span class="add">
                            <button type="button" class="btn btn-light">Add</button>
                            <span class="drafted">
                                <button type="button" class="btn btn-light">Drafted</button>
                            </span>
                        </span>
                    </span>
                </div>
            </div>
        </div>
    </section>
    </div>


